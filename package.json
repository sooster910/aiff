    "@slack/web-api": "^6.7.2",
    "@tosspayments/payment-sdk": "^1.1.0",
    "@types/mixpanel": "^2.14.9",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "date-fns": "^2.28.0",
    "formik": "^2.2.9",
    "got": "11.0.0",
