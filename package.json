{
  "name": "aiff-front",
  "version": "0.1.0",
  "private": true,
  "node": ">=16.0.0",
  "scripts": {
    "generate": "get-graphql-schema http://localhost:3434/graphql > schema.graphql",
    "serve-json": "json-server --watch db.json --port 9000",
    "relay": "relay-compiler",
    "dev": "nodemon ./src/server.js",
    "develop": "next",
    "dev-json": "yarn run next dev",
    "lint": "next lint",
    "clean": "rm -rf .next",
    "start": "node ./src/server.js start -p 8081",
    "build": "yarn run clean && next build",
    "deploy-dev": "yarn install && yarn run build && eb deploy production-aiff-front",
    "test": "jest"
  },
